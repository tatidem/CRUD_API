{
  "name": "simple-crud-api",
  "version": "1.0.0",
  "description": "Simple CRUD API with in-memory database",
  "main": "src/main.ts",
  "scripts": {
    "start:dev": "nodemon",
    "start:multi": "nodemon -- --multi",
    "start:prod": "webpack --mode=production --node-env=production && node dist/server-prod.js",
    "start:prod:multi": "webpack --mode=production --node-env=production && node dist/server-prod.js --multi",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check \"src/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.17",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=22.14.0"
  }
}
